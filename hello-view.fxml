<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="219.0" prefWidth="326.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/17.0.6"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.caculadora.HelloController">

    <padding>
        <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
    </padding>

    <Button fx:id="btnSumar" onAction="#irASumar" text="Sumar"/>
    <Button fx:id="btnRestar" onAction="#irARestar" text="Restar"/>
    <Button fx:id="btnDividir" onAction="#irADividir" text="Dividir"/>
    <Button fx:id="btnMultiplicar" onAction="#irAMultiplicar" text="Multiplicar"/>
</VBox>